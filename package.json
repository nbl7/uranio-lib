{
  "name": "urn-lib",
  "version": "0.0.1",
  "description": "Uranio library. Shared code used in Uranio repos.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "node -r source-map-support/register dist/index.js",
    "build": "tsc -b",
    "dev": "./node_modules/.bin/tsc-watch --onSuccess \"node -r source-map-support/register ./dist/dev.js\"",
    "dev:browser": "npx webpack serve --open"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:nbl7/uranio-lib.git"
  },
  "keywords": [
    "uranio",
    "urn"
  ],
  "author": "Andrea Reni <mail@nbl7.net>",
  "license": "ISC",
  "homepage": "https://github.com/nbl7/uranio-lib#readme",
  "devDependencies": {
    "ts-loader": "^9.2.6",
    "uranio-dev": "git+ssh://git@github.com:nbl7/uranio-dev.git",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0"
  },
  "dependencies": {
    "dateformat": "^3.0.3"
  }
}
